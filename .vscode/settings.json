{
  "typescript.tsdk": "node_modules/typescript/lib",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": false
  },
  "vsicons.presets.nestjs": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#06b9a5",
    "activityBar.foreground": "#06b9a5",
    "activityBar.inactiveForeground": "#f9fb97",
    "activityBarBadge.background": "#06b9a5",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#f9fb97",
    "statusBar.background": "#f9fb97",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#f9fb97",
    "statusBarItem.remoteBackground": "#f9fb97",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#f9fb97",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ff5dfc7c",
    "titleBar.inactiveForeground": "#fcfcfc"
  }
}
